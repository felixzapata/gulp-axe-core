{
  "extends": "eslint:recommended",
  "rules": {
    "no-console": "off"
  },
  "env": {
    "browser": true
  },
  "plugins": [
    "html"
  ],
  "globals": {
    "require" : false,
    "__dirname" : false,
    "describe" : false,
    "process" : false,
    "beforeEach" : false,
    "afterEach" : false,
    "it" : false
  }
}